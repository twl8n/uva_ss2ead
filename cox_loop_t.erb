
<c0<%=old_href['component']%> level="<%= old_href['c0x level'] %>">
  <did>
    <unittitle <%= old_href['label_series'] %>><%=old_href['unittitle']%>
      <unitdate type="inclusive"><%=old_href['unitdate']%></unitdate>
    </unittitle>
    <physloc><%=old_href['physloc']%></physloc>
    <% if old_href['container_flag'] %>
    <container label="<%= old_href['container_label'] %>" type="<%= old_href['container_type'] %>">
      <%=old_href['number']%>
    </container>
    <% end %>
    <physdesc><%=old_href['physdesc']%></physdesc>
  </did>
  <scopecontent>
    <% if old_href['c_num'].to_s.empty? %>
    <num><%= old_href['c_num'] %></num>
    <% end %>
    <p><%= old_href['scopecontent'] %></p>
  </scopecontent>
  <arrangement>
    <p/>
  </arrangement>
  <%= content %>
</c0<%= old_href['component'] %>>

